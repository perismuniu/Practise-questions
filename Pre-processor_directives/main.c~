#include <stdio.h>
#include "main.h"

/**
 *main - Entry point.
 *Compute Arithmetic functions - Sum, sub, mult, div, mod
 *Return : Always (0)
 */

int main(void)
{
printf("sum = %d", sum(100, 50));
printf("sub = %d", sub(100, 50));
printf("mult = %d", mult(100, 50));
printf("div = %d", div(100, 50));
printf("mod = %d", mod(100, 50));

return (0);
}
